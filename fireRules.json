{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
            "$contact": {
				".validate" : "newData.hasChildren(['name', 'busiType', 'busiNum', 'address', 'prov'])",
                "name": {
                    ".validate": "newData.isString() && newData.val().length < 49 && newData.val().length > 1"
                },
                "busiNum": {
                    ".validate": "newData.isString() && newData.val().length = 9"
                }
				"busiType": {
                    ".validate": "newData.isString() && newData.val().length = 9"
                }
            }
        }
  }
}